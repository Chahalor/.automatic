{
	"HeaderFileType": {
		"prefix": "typeHeader",
		"description": "standard header for types headers",
		"body": [
			"// Header",
			"",
			"# pragma once",
			"",
			"/* ************************************************************************** */",
			"/*                                 Headers                                    */",
			"/* ************************************************************************** */",
			"",
			"/* -----| Systems   |----- */",
			"\t//...",
			"",
			"/* -----| Globals   |----- */",
			"\t//...",
			"",
			"/* -----| Internals |----- */",
			"\t//...",
			"",
			"/* -----| Modules   |----- */",
			"\t//...",
			"",
			"/* ************************************************************************** */",
			"/*                                 Typedefs                                   */",
			"/* ************************************************************************** */",
			"\t//...",
			"",
			"/* ************************************************************************** */",
			"/*                                 Enums                                      */",
			"/* ************************************************************************** */",
			"\t//...",
			"",
			"/* ************************************************************************** */",
			"/*                                 Unions                                     */",
			"/* ************************************************************************** */",
			"\t//...",
			"",
			"/* ************************************************************************** */",
			"/*                                 Structs                                    */",
			"/* ************************************************************************** */",
			"\t//..."
		]
	},
	"HeaderFilePublic": {
		"prefix": "interfaceHeader",
		"description": "standard header for interface headers",
		"body": [
			"// Header",
			"",
			"# pragma once",
			"",
			"/* ************************************************************************** */",
			"/*                                 Headers                                    */",
			"/* ************************************************************************** */",
			"",
			"/* -----| Systems   |----- */",
			"\t//...",
			"",
			"/* -----| Globals   |----- */",
			"# include \"config.h\"",
			"# include \"standards.h\"",
			"",
			"/* -----| Internals |----- */",
			"# include \"types.h\"",
			"",
			"/* -----| Modules   |----- */",
			"\t//...",
			"",
			"/* ************************************************************************** */",
			"/*                                 Macros                                     */",
			"/* ************************************************************************** */",
			"\t//...",
			"",
			"/* ************************************************************************** */",
			"/*                                 Prototypes                                 */",
			"/* ************************************************************************** */",
			"\t//..."
		]
	},
	"cFile": {
		"prefix": "cFile",
		"description": "standard header for c files",
		"body": [
			"// Header",
			"",
			"/* ----| Headers    |----- */",
			"\t/* Standard */",
			"\t\t//...",
			"\t/* Internal */",
			"\t\t//...",
			"\t/* External */",
			"\t\t//...",
			"",
			"/* ----| Prototypes |----- */",
			"\t//...",
			"",
			"/* ----| Internals  |----- */",
			"\t//...",
			"",
			"/* ----| Public     |----- */",
			"\t//...",
			"$2"
		],
	},
	"ifndef": {
		"prefix": "ifndef",
		"description": "snippet for faster ifndef statement",
		"body": [
			"#ifndef $1",
			"# define $1",
			"#endif //!$1"
		]
	}
}
